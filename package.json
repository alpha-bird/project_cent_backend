  "scripts": {
    "start": "npm run build && node ./build",
    "local": "npm run build && npm run local:nodemon",
    "local:nodemon": "tsc --project './tsconfig.json' --watch & APP_ENV=local nodemon ./build",
    "local:docker": "npm run clean && docker-compose up --build",
    "build": "npm run build:bundle && tsc --project './tsconfig.json'",
    "build:bundle": "browserify -d ./views/main.js -o ./public/bundle.js -t [ babelify --presets [ @babel/preset-env ] ] --extension=jsx --extension=js",
    "dev:watch": "tsc-watch --preserveWatchOutput -p tsconfig.json --onSuccess 'node ./build'",
    "prebuild": "npm run clean && npm run lint && mkdir .logs",
    "lint": "eslint . --ext .ts,.tsx",
    "clean": "rm -rf build tmp .logs",
    "db:sync": "sh ./scripts/db-sync.sh",
    "test": "jest --coverage",
    "test:watch": "jest --watch"
  },
